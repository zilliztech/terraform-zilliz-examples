{
  "name": "zilliz-byoc-azure",
  "location": "East US",
  "resource_group_name": "byoc-test",
  "tags": {
    "Environment": "Production",
    "Project": "Zilliz-BYOC",
    "Owner": "Zilliz-Infra"
  },
  "vnet": {
    "cidr": "10.0.0.0/16"
  },
  "storage": {
    "name": "zillizbyocstorage"
  },
  "enable_private_link": false,
  "private_endpoint": {
    "name": "storage-pe"
  },
  "aks": {
    "node_pools": [
      {
        "name": "core",
        "vm_size": "Standard_D8as_v5",
        "node_count": 2,
        "min_count": 2,
        "max_count": 2,
        "enable_auto_scaling": true,
        "os_disk_size_gb": 256,
        "os_disk_type": "Managed",
        "node_labels": {
          "zilliz-group-name": "core",
          "node-role/etcd": "true",
          "node-role/pulsar": "true",
          "node-role/infra": "true",
          "node-role/vdc": "true",
          "node-role/milvus-tool": "true",
          "capacity-type": "ON_DEMAND"
        },
        "node_taints": []
      },
      {
        "name": "search",
        "vm_size": "Standard_L16s_v3",
        "node_count": 3,
        "min_count": 1,
        "max_count": 20,
        "enable_auto_scaling": true,
        "os_disk_size_gb": 160,
        "os_disk_type": "Ephemeral",
        "node_labels": {
          "zilliz-group-name": "search",
          "node-role/diskANN": "true",
          "node-role/milvus": "true",
          "node-role/nvme-quota": "200"
        },
        "node_taints": []
      },
      {
        "name": "index",
        "vm_size": "Standard_D8s_v5",
        "node_count": 3,
        "min_count": 1,
        "max_count": 60,
        "enable_auto_scaling": true,
        "os_disk_size_gb": 256,
        "os_disk_type": "Managed",
        "node_labels": {
          "zilliz-group-name": "index",
          "node-role/index-pool": "true"
        },
        "node_taints": []
      },
      {
        "name": "fundamental",
        "vm_size": "Standard_D4as_v5",
        "node_count": 0,
        "min_count": 0,
        "max_count": 6,
        "enable_auto_scaling": true,
        "os_disk_size_gb": 128,
        "os_disk_type": "Managed",
        "node_labels": {
          "zilliz-group-name": "fundamental",
          "node-role/default": "true",
          "node-role/milvus": "true"
        },
        "node_taints": []
      }
    ]
  },
  "network_security": {
    "enable_public_support": false,
    "allowed_subnets": ["milvus"]
  },
  "auth_token": "xxxxxxxxxxxx",
  "env": "uat",
  "dataplane_id": "xxxxxx",
  "project_id": "xxxx",
  "customer_acr": {
    "name": "xxxx",
    "prefix": "zilliz-byoc"
  }
}
